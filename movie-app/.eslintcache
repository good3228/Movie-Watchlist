nt.tagline && (\n                    <i className=\"tagline\">{content.tagline}</i>\n                  )}\n\n                  <span className=\"ContentModal__description\">\n                    {content.overview}\n                  </span>\n\n                  <Button\n                    variant=\"contained\"\n                    startIcon={<YouTubeIcon />}\n                    color=\"secondary\"\n                    style={{\n                      backgroundColor: \" #c4302b\",\n                      fontSize: \" 1.2rem\",\n                    }}\n                    target=\"__blank\"\n                    href={`https://www.youtube.com/watch?v=${video}`}\n                  >\n                    Watch the Trailer\n                  </Button>\n                </div>\n              </div>\n            </div>\n          )}\n        </Fade>\n      </Modal>\n    </>\n  );\n}\n",{"ruleId":"94","replacedBy":"95"},{"ruleId":"96","replacedBy":"97"},{"ruleId":"98","severity":2,"message":"99","line":22,"column":29,"nodeType":"100","messageId":"101","endLine":22,"endColumn":37},{"ruleId":"102","severity":1,"message":"103","line":20,"column":7,"nodeType":"104","endLine":23,"endColumn":8},{"ruleId":"105","severity":1,"message":"106","line":2,"column":10,"nodeType":"100","messageId":"107","endLine":2,"endColumn":20},{"ruleId":"105","severity":1,"message":"108","line":14,"column":7,"nodeType":"100","messageId":"107","endLine":14,"endColumn":16},{"ruleId":"109","severity":2,"message":"110","line":129,"column":20,"nodeType":"111","messageId":"112","endLine":129,"endColumn":26},{"ruleId":"109","severity":2,"message":"113","line":131,"column":33,"nodeType":"111","messageId":"112","endLine":131,"endColumn":44},{"ruleId":"105","severity":1,"message":"106","line":2,"column":10,"nodeType":"100","messageId":"107","endLine":2,"endColumn":20},{"ruleId":"105","severity":1,"message":"108","line":17,"column":7,"nodeType":"100","messageId":"107","endLine":17,"endColumn":16},"no-native-reassi